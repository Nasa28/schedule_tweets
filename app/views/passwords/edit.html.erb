<h1>Edit Password</h1>

<%= form_with  url: edit_password_path do |form|%>
 
   <div class="mb-3">
  <%= form.label :email%>
  <%= form.text_field :email, class: "form-control", placeholder: "example@gmail.com"%>
  </div>
  <div class="mb-3">
  <%= form.label :password%>
  <%= form.password_field :password, class: "form-control", placeholder: "password"%>
  </div>
  <div class="mb-3">
  <%= form.submit "Sign In", class: "btn btn-primary" %>
  </div>
<%end%>